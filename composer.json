{
  "name": "coffeebreaks/database",
  "description": "This component connects, registers, reads, edits and deletes records in the database",
  "license": "MIT",
  "authors": [
    {
      "name": "Guilherme K. Fagundes",
      "role": "System Analist / Web Developer"
    }
  ],
  "type": "library",
  "keywords": [
    "Connect",
    "Insert",
    "Read",
    "Delete",
    "Update",
